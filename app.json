{
  "expo": {
    "name": "NewDarts",
    "slug": "newdarts",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#16171d"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.rgehrke.newdarts",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/icon.png",
        "backgroundColor": "#16171d"
      },
      "package": "com.rgehrke.newdarts"
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-secure-store"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "c3fc49fb-a9b6-4d4e-ae98-615c972e5f21"
      }
    },
    "owner": "rgehrke",
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/c3fc49fb-a9b6-4d4e-ae98-615c972e5f21"
    }
  }
}
